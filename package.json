{
  "name": "tinybuild-homepage-test",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "eleventy --serve --incremental --quiet",
    "build": "bun run clean && NODE_ENV=production eleventy",
    "build:dev": "bun run clean && NODE_ENV=development eleventy",
    "preview": "eleventy --serve --port 4173 --input _site",
    "clean": "rm -rf _site .11ty-vite"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^4.0.0-alpha.4",
    "@11ty/eleventy-navigation": "^1.0.4",
    "@11ty/eleventy-img": "^7.0.0-alpha.2",
    "@11ty/is-land": "^5.0.0",
    "eleventy-plugin-vento": "^5.0.0",
    "@11ty/eleventy-plugin-vite": "^7.1.0-alpha.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "vite-plugin-simple-html": "^1.0.1",
    "svelte": "^5.38.10",
    "lightningcss": "^1.30.1",
    "browserslist": "^4.26.0",
    "@biomejs/biome": "2.2.4",
    "@types/node": "^24.4.0",
    "typescript": "^5.9.2"
  },
  "browserslist": ["defaults and supports es6-module"],
  "engines": {
    "bun": ">=1.2.0"
  }
}